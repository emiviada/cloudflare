{
    "name": "emiviada/cloudflare",
    "description": "A PHP sdk to communicate with CloudFlare api V4",
    "keywords": ["CloudFlare", "API", "SDK"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Emiliano Viada",
            "email": "emjovi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/options-resolver": "^2.7 || ^3.0",
        "guzzlehttp/guzzle": "^4.0 || ^5.0 || ^6.0.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "EmiViada\\Cloudflare\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "EmiViada\\Cloudflare\\Tests\\": "tests" }
    }
}